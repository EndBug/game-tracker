{
  "name": "game-tracker",
  "version": "0.0.0",
  "description": "A Discord bot that helps you to track your stats in many games.",
  "author": "Federico Grandi <fgrandi30@gmail.com>",
  "license": "MIT",
  "main": "src/core/start.ts",
  "scripts": {
    "start": "ts-node src/core/start.ts",
    "beta": "cross-env NODE_ENV=beta && npm start",
    "update": "git pull && npm ci",
    "dev": "cross-env NODE_ENV=dev nodemon --watch src/**/*.* --ignore src/utils/reloadme.json --exec npm start",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "docs:add": "npm run docs:gen && git add ./docs",
    "docs:dev": "docsify serve ./docs",
    "docs:gen": "ts-node src/scripts/doc.ts",
    "lint:add": "npm run lint && git add ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintIgnore": [
    "src/utils/reloadme.json"
  ],
  "dependencies": {
    "@supabase/supabase-js": "^1.29.2",
    "axios": "^0.25.0",
    "common-tags": "^1.8.2",
    "cron": "^1.8.2",
    "dbots": "^11.0.0",
    "discord.js-light": "^4.3.1",
    "dotenv": "^14.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-rev-sync": "^3.0.1",
    "nodemon": "^2.0.4",
    "overwatch-stats-api": "^2.1.0",
    "prettier": "^2.5.1",
    "pretty-error": "^4.0.0",
    "r6api.js": "^4.0.0",
    "statcord.js": "^3.4.1",
    "ts-node": "10.4.0",
    "typescript-map": "0.1.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cron": "^1.7.3",
    "@types/markdown-table": "^2.0.0",
    "@types/node": "^12.12.62",
    "@types/nodemon": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "all-contributors-cli": "^6.20.0",
    "cross-env": "^7.0.3",
    "docsify-cli": "^4.4.3",
    "eslint": "^7.32.0",
    "markdown-table": "^2.0.0",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndBug/game-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/EndBug/game-tracker/issues"
  },
  "homepage": "https://github.com/EndBug/game-tracker#readme"
}
