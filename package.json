{
  "name": "game-tracker",
  "version": "0.0.0",
  "description": "A Discord bot that helps you to track your stats in many games.",
  "author": "Federico Grandi <fgrandi30@gmail.com>",
  "license": "MIT",
  "main": "src/core/start.ts",
  "scripts": {
    "start": "ts-node src/core/start.ts",
    "beta": "cross-env NODE_ENV=beta && npm start",
    "update": "git pull && npm ci",
    "dev": "cross-env NODE_ENV=dev nodemon --watch src/**/*.* --ignore src/utils/reloadme.json --exec npm start",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc",
    "test": "npm run build && npm run lint"
  },
  "eslintIgnore": [
    "src/utils/reloadme.json"
  ],
  "dependencies": {
    "@discordjs/rest": "^1.2.0",
    "@endbug/overwatch-stats-api": "^3.0.1",
    "@supabase/supabase-js": "^1.35.7",
    "blapi": "^2.2.4",
    "cron": "^2.1.0",
    "discord.js": "^14.0.2",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-rev-sync": "^3.0.1",
    "match-sorter": "^6.3.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.7.1",
    "pretty-error": "^4.0.0",
    "r6api.js": "^4.4.1",
    "require-all": "^3.0.0",
    "statcord.js": "^3.4.3",
    "ts-node": "10.9.1",
    "typescript-map": "0.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cron": "^2.0.0",
    "@types/node": "^18.0.6",
    "@types/nodemon": "^1.19.2",
    "@types/require-all": "^3.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "all-contributors-cli": "^6.20.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndBug/game-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/EndBug/game-tracker/issues"
  },
  "homepage": "https://github.com/EndBug/game-tracker#readme"
}
