{
  "name": "game-tracker",
  "version": "0.0.0",
  "description": "A Discord bot that helps you to track your stats in many games.",
  "main": "src/core/start.ts",
  "scripts": {
    "start": "ts-node src/core/start.ts",
    "update": "git pull && npm i",
    "update:custom": "npm i EndBug/Commando#djs-v11.4.2",
    "update:force": "git checkout -- package-lock.json && npm run update",
    "dev": "nodemon --watch src/**/*.* --ignore src/commands/**/*.* --ignore src/utils/reloadme.json --exec ts-node src/core/start.ts",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "doc": "ts-node src/scripts/doc.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Federico 'EndBug' Grandi <fgrandi30@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dbots": "^1.2.3",
    "discord.js": "11.4.2",
    "discord.js-commando": "github:EndBug/Commando#djs-v11.4.2",
    "dotenv": "^6.2.0",
    "markdown-table": "^1.1.3",
    "nodemon": "^1.18.10",
    "overwatch-stats-api": "^2.0.6",
    "request-promise": "^4.2.4",
    "sqlite": "^3.0.2",
    "ts-node": "^8.0.3",
    "typescript-map": "0.0.7"
  },
  "devDependencies": {
    "@types/node": "^11.10.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.6.0",
    "all-contributors-cli": "^6.3.1",
    "eslint": "^6.2.2",
    "husky": "^3.0.4",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run doc"
    }
  }
}